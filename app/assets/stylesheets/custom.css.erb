/********** STYLE MISLEY  ************/

html{
  font-family: 'ProximaNova-Light'!important;
 }
body{
  background: url('<%= asset_path("hd-bg.png") %>');
  background-repeat: no-repeat;
  background-size: cover;
  color: #808080;
 }
h1, h2, h3, h4, h5 {
  font-weight: lighter!important;
  color: #808080 !important;
  margin: 0.82rem 0 2rem!important;
}
/*#content{
   padding-top: 60px;
 }*/
.signed_user {
  margin-top: 40px;
}
#stand-up header{
    padding-bottom: 8vh;
}
#stand-up nav{
  background-color: transparent;
  box-shadow: none;
  -moz-box-shadow: none;
  -webkit-box-shadow: none;
}
#stand-up nav .brand-logo img{
  display: inline-table;
  vertical-align: middle;
}
#stand-up nav a{
  text-transform: uppercase;
}
#stand-up nav .menu a,
#stand-up nav .dropdown-button,
#stand-up nav .dropdown-content li > a,
#stand-up nav .dropdown-content li > span{
  font-size: 0.9rem;
}
#stand-up nav .menu a,
#stand-up nav a.dropdown-button:visited {
    color: #fff;
}
#stand-up nav .menu a {
    padding: 0 20px;

}
#stand-up nav .dropdown-button{
  margin-left: 2rem;
}
#stand-up nav .dropdown-content {
  min-width: 110px;
}
#stand-up nav .dropdown-content li > a,
#stand-up nav .dropdown-content li > span{
  line-height: 22px;
  text-align: center;
}
#stand-up nav .dropdown-content li > a{
  color: #808080;
}
#stand-up nav ul.menu a:hover,
#stand-up nav .dropdown-content li > a:hover {
  background-color: none;

}
#stand-up nav .avatar-icons{
  display: table;
  height: 70px;
}
#stand-up nav .avatar-icons img {
  background-color: #fff;
  border-radius: 5px;
  display: inline-table;
  vertical-align: middle;
  padding: 5px;
}
/* STYLE FOR CARD ELEMENT*/
.card {
  margin: 0 auto!important;
  padding: 1.5rem 0.6rem 1.5rem 0.8rem;
  border-radius: 5px!important;
  -moz-border-radius: 5px;
  /* for Safari y Chrome*/
  -webkit-border-radius: 5px;

}
.card-title .row {
  margin: 0!important;
}
div .card{
  box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);
  -moz-box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);
  -webkit-box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);

}
.card .card-title {
  border-bottom: 1px solid rgba(160,160,160,0.2);
  min-height: 100px;
}
.card .card-title h1{
  margin-top: 10px;
  margin-bottom: 0px;
  font-size: 2rem!important;
  text-transform: uppercase;
  color: #12aaeb!important;
  line-height: 2rem;
  margin: 0px!important;
}
.card .card-title h6{
  margin-top: 0.4rem!important;
  color: #94a8ba;
  text-transform: uppercase;
}
.container .card .card-action a:not(.btn):not(.btn-large):not(.btn-floating) {
  color: #989797;
}
.container .card .card-action a:not(.btn):not(.btn-large):not(.btn-floating):hover {
  color: #12aaeb;
}
/* END STYLE FOR CARD ELEMENT */
/* STYLE FOR BUTTON*/
.su-btn.btn-flat {
  color: #989797;
  background-color: #fff;
  border: 1px solid #12aaeb!important;
  border-radius: 5px;
  -moz-border-radius: 5px;
  /* for Safari y Chrome*/
  -webkit-border-radius: 5px;


}
.su-btn input{
  border:0px;
}
/* END STYLE FOR BUTTON */
/* STYLE FOR INPUT */
/** CHANGE COLOR IN FOCUS (MATERIALIZE) **/
.container input:not([type]):focus:not([readonly]),
.container input[type=text]:focus:not([readonly]),
.container input[type=password]:focus:not([readonly]),
.container input[type=email]:focus:not([readonly]),
.container input[type=url]:focus:not([readonly]),
.container input[type=time]:focus:not([readonly]),
.container input[type=date]:focus:not([readonly]),
.container input[type=datetime]:focus:not([readonly]),
.container input[type=datetime-local]:focus:not([readonly]),
.container input[type=tel]:focus:not([readonly]),
.container input[type=number]:focus:not([readonly]),
.container input[type=search]:focus:not([readonly]),
.container textarea.materialize-textarea:focus:not([readonly]) {
  border-bottom: 1px solid #12aaeb;
  box-shadow: 0 1px 0 0 #12aaeb;
  -moz-box-shadow: 0 1px 0 0 #12aaeb;
  /* for Safari y Chrome*/
  -webkit-box-shadow: 0 1px 0 0 #12aaeb

}

.container input:not([type]):focus:not([readonly]) + label,
.container input[type=text]:focus:not([readonly]) + label,
.container input[type=password]:focus:not([readonly]) + label,
.container input[type=email]:focus:not([readonly]) + label,
.container input[type=url]:focus:not([readonly]) + label,
.container input[type=time]:focus:not([readonly]) + label,
.container input[type=date]:focus:not([readonly]) + label,
.container input[type=datetime]:focus:not([readonly]) + label,
.container input[type=datetime-local]:focus:not([readonly]) + label,
.container input[type=tel]:focus:not([readonly]) + label,
.container input[type=number]:focus:not([readonly]) + label,
.container input[type=search]:focus:not([readonly]) + label,
.container textarea.materialize-textarea:focus:not([readonly]) + label {
  color: #12aaeb;
}
.container .input-field .prefix.active {
  color: #12aaeb;
}
/** END CHANGE COLOR IN FOCUS (MATERIALIZE) **/

input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
  background: none !important;
  background-image: none;
  color: rgb(0, 0, 0);
}
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus,
input:-webkit-autofill:active,
textarea:-webkit-autofill,
textarea:-webkit-autofill:hover,
textarea:-webkit-autofill:focus,
textarea:-webkit-autofill:active,
select:-webkit-autofill,
select:-webkit-autofill:hover,
select:-webkit-autofill:focus,
select:-webkit-autofill:active{
  transition: background-color 5000s ease-in-out 0s;
  -webkit-box-shadow: 0 0 0px 1000px #fff inset;
}
textarea:focus, input:focus, select:focus{
  outline: none;
}
/* END STYLE FOR INPUT */
a:hover{
  background: none!important;
}

i.small {
  font-size: 22px !important;
}
.div-right:after {
    content: "";
    font-size: 1.5rem;
    line-height: 20px;
    vertical-align: middle;
}
.border-right, .div-right:after{
  border-right: 1px solid #9c9c9c;
}
.border-bottom{
  border-bottom: 1px solid #9c9c9c;
}
.notice{
  text-align: center;
  line-height: 2em;
}
.danger{
  text-align: center;
  font-style: italic;
  line-height: 3em;
  color: #f77f5f;
}
/* MEDIA SCREEN */
@media only screen and (min-width: 601px){
 #stand-up nav, #stand-up nav .nav-wrapper i, #stand-up nav a.button-collapse, #stand-up nav a.button-collapse i {
   height: 85px;
   line-height: 85px;
 }
}
